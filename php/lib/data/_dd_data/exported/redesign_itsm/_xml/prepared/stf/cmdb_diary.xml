							<![CDATA[OnFormLoaded()
{
	//--F0085885
 	var boolUpdating = (_swdoc.oDiary().pk_auto_id>0);
 	if(!boolUpdating)
		_swdoc.oDiary().updatedonx = app.global.GetCurrentEpocTime();
	
	app.g.popuplate_form_fromargs("config_diary", _swdoc, true,true);
	//--eo F0085885

 	app._ero(tb_ad, boolUpdating);
 	app._ero(tb_ac, boolUpdating);
	app._ero(tb_as, boolUpdating);
 	app._ero(tb_ab, boolUpdating);
	app._ero(tb_desc, boolUpdating);
	
	if(_swdoc.GetArg("service"))
	{
		app._ete(_swdoc.MF().fld_id_label, "Service ID");
	}
	
	if(boolUpdating)
	{
		_swdoc.SetTitle("Diary Entry (Readonly)");
	}
	else
	{
		_swdoc.SetTitle("Add New Diary Entry");
	}
	
}]]>
