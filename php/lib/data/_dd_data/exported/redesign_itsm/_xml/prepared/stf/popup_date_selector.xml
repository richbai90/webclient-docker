			<formActions>
			</formActions>
			<javascript>
				<methods>
					<method>
						<name>OnFormLoaded</name>
						<code>
							<![CDATA[OnFormLoaded(bDataReloaded)
{
	//-- if passed in lable filed name use in label
	var strFieldName = _swdoc.GetArg("label_name");
	if(strFieldName!="")
	{
		app._ete(lbl_date , "Select " + strFieldName + " dates between");
	}
	else
	{
		app._ete(lbl_date , "Select dates between");
	}


}]]>
						</code>
					</method>
					<method>
						<name>dt_two_OnValueChanged</name>
						<code>
							<![CDATA[dt_two_OnValueChanged(strValue)
{
	
	if((dt_two.value<dt_one.value)||(dt_one.value==0))app._eva(dt_one , dt_two.value);


}]]>
						</code>
					</method>
					<method>
						<name>dt_one_OnValueChanged</name>
						<code>
							<![CDATA[dt_one_OnValueChanged(strValue)
{
	if(dt_two.value<dt_one.value)app._eva(dt_two , dt_one.value);

}]]>
						</code>
					</method>
					<method>
						<name>btn_select_OnPressed</name>
						<code>
							<![CDATA[btn_select_OnPressed()
{
	_swdoc.fromDate = dt_one.value + "|" + dt_one.text;
	_swdoc.toDate = dt_two.value + "|" + dt_two.text;
	_swdoc.CloseForm();	
}]]>
						</code>
					</method>
				</methods>
