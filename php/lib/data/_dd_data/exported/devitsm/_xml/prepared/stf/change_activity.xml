				<method>
					<name>OnPreValidate</name>
					<code>
						<![CDATA[OnPreValidate()
{
	// TODO: Add your event handler code here
	var oSched = new Object();
	oSched.startDatex = app.g.fix_epoch(_swdoc.Sched().startx);
	oSched.endDatex = app.g.fix_epoch(_swdoc.Sched().endx);
	oSched.pk_auto_id = _swdoc.Sched().pk_auto_id;
	oSched.title = _swdoc.Sched().title;

	var strWarning = "";
	if(oSched.endDatex<1)
		strWarning = "The field 'End On' is a mandatory field and must be completed";
	if(oSched.startDatex<1)
		strWarning = "The field 'Start On' is a mandatory field and must be completed";

	if(oSched.endDatex<oSched.startDatex)
	{
		strWarning = "End date cannot be before the start date.";
	}

	if(strWarning!="")
	{
		
		MessageBox(strWarning,16);
		return VALIDATE_FAIL;
	}


	var oActs = new Array();
	oActs.push(oSched);
	var strAlertMessage = app.cmdb.check_activity_schedule(_swdoc.GetArg("ci_ids"),oActs,true,_swdoc.GetArg("requireBy"), _swdoc.Sched().fk_callref);
	if(strAlertMessage!="")
	{
		var boolConfirm = confirm(strAlertMessage+"\n\nDo you wish to continue?");
		if(!boolConfirm)
			return VALIDATE_FAIL;
	}

	return VALIDATE_DEFAULT;
}]]>
					</code>
				</method>
