				<method>
					<name>OnSaveData</name>
					<code>
						<![CDATA[OnSaveData()
{
	// TODO: Add your event handler code here
	if(in_array(_swdoc.monthArray,_swdoc.tm().from_mm))
	{
		if(_swdoc.tm().from_mm==="2")
		{
			if(_swdoc.tm().from_dd>28)
			{
				MessageBox('Please select the from date less than 29 days.');
				return false;
			}
		}else
		{
			if(_swdoc.tm().from_dd>30)
			{
				MessageBox('Please select the from date less than 31 days.');
				return false;
			}
		}
	}

	if(in_array(_swdoc.monthArray,_swdoc.tm().to_mm))
	{
		if(_swdoc.tm().to_mm==="2")
		{
			if(_swdoc.tm().to_dd>28)
			{
				MessageBox('Please select the to date less than 29 days.');
				return false;
			}
		}else
		{
			if(_swdoc.tm().to_dd>30)
			{
				MessageBox('Please select the to date less than 31 days.');
				return false;
			}
		}
	}
	return true;
}]]>
