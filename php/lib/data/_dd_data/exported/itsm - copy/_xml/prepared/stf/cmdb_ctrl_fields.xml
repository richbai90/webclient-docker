	}
	lb_labels.pickList = strLbData;
	if(_swdoc.oAtt().fk_config_type=="")
	{
		
		_swdoc.oAtt().fk_config_type = unescape(_swdoc.GetArg("fk_config_type")); //F0108069
		_swdoc.oAtt().flg_sc = _swdoc.GetArg("flg_sc");
