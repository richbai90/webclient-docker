					<method>
						<name>OnFormLoaded</name>
						<code>
							<![CDATA[OnFormLoaded(bDataReloaded)
{

	var strPeriodName = _swdoc.GetArg("period_name");
	if ("" != strPeriodName){
		_swdoc.OHN().onhold_period = strPeriodName;
	}
	var b = app.g.get_record("onhold_periods", _swdoc.OHN().onhold_period);
	_swdoc.max_days = b.period_days;

	var e_template = _swdoc.OHN().template;
	if(""!=_swdoc.OHN().mailbox){
		_swdoc.MF().lb_mailbox_OnValueChanged(_swdoc.OHN().mailbox);
		_swdoc.OHN().template = e_template;
	}
	_swdoc.UpdateFormFromData();
	_swdoc.ResetModiedFlag("onhold_notifiers");
	

}]]>
						</code>
					</method>
					<method>
						<name>lb_mailbox_OnValueChanged</name>
						<code>
							<![CDATA[lb_mailbox_OnValueChanged(strValue)
{


	if("" != strValue){
		var strURL = "php/itsm/get_templatelist.php?mailboxname=" + lb_mailbox.text; 
		var strResult = app.g.submit_php(strURL,true);
		_swdoc.MF().lb_template.pickList = strResult;
	}
	_swdoc.OHN().template = "";
	_swdoc.UpdateFormFromData();


}]]>
						</code>
					</method>
					<method>
						<name>lb_mailbox_OnLeftClick</name>
						<code>
							<![CDATA[lb_mailbox_OnLeftClick()
{
	//-- get mailbox list
	
	if(lb_mailbox.pickList=="")
	{
		//-- call php to get mailbox list
		var strURL = "php/itsm/get_mailboxlist.php"; 
		var strResult = app.g.submit_php(strURL,true);
		lb_mailbox.pickList = strResult;
	}


	return true;
}]]>
						</code>
					</method>
					<method>
						<name>fld_days_OnValueChanged</name>
						<code>
							<![CDATA[fld_days_OnValueChanged(strValue)
{

	if (strValue < 0 || strValue > _swdoc.max_days){
		MessageBox("You specified more days than available in this period.");
		_swdoc.OHN().days_from = _swdoc.max_days;
		_swdoc.UpdateFormFromData();
	}

}]]>
