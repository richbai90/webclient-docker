					<name>OnPreValidate</name>
					<code>
						<![CDATA[OnPreValidate()
{
	var oNotif = new Object();
	oNotif.startDatex = app.g.fix_epoch(wss_notif.startdatex);
	oNotif.endDatex = app.g.fix_epoch(wss_notif.enddatex);
	var strWarning = "";
	if(oNotif.endDatex<1)
		strWarning = "The field 'End Date' is a mandatory field and must be completed.";
	if(oNotif.startDatex<1)
		strWarning = "The field 'Start Date' is a mandatory field and must be completed.";

	if(oNotif.endDatex<oNotif.startDatex && oNotif.endDatex>=1)
	{
		strWarning = "End date cannot be before the start date.";
	}

	if(strWarning!="")
	{
		
		MessageBox(strWarning,16);
		return VALIDATE_FAIL;
	}

	return VALIDATE_DEFAULT;
}]]>
