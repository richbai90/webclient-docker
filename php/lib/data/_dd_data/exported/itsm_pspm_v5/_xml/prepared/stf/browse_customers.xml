					<method>
						<name>OnFormLoaded</name>
						<code>
							<![CDATA[OnFormLoaded(bDataReloaded)
{
	//-- set title
	if(_swdoc.GetArg("in_mode")=="search" || _swdoc.GetArg("in_mode")=="")
	{
		_evi(btn_select, true);
		_swdoc.SetTitle("Browse Customers");
	}
	else
	{
		_evi(btn_select, false);
		_swdoc.SetTitle("Manage Customers");
	}
	
	//-- so can filter when popped up from company form for example
	_swdoc.PassedInFilter = _swdoc.GetArg("in_filter");
	
	//-- show / hide del and add
	_evi(btn_add, (!btn_select.visible));
	_evi(btn_del, (!btn_select.visible));
	_evi(btn_prop, (!btn_select.visible));
	
	_slf(sl_browse, _swdoc.PassedInFilter);
	sl_browse.SetRowSelected(0);
	
	//-- load pick list
	var strPickList = "";
	for(var x = 0; x < sl_browse.colCount; x++)
	{
		strPickList += sl_browse.GetColumnDisplayName(x) + "^" + x + "|";
		
	}
	lb_columns.pickList = strPickList;

}]]>
						</code>
					</method>
					<method>
						<name>btn_find_OnPressed</name>
						<code>
							<![CDATA[btn_find_OnPressed()
{
	//--
	
	var strVal = tb_lookfor.text;
	var strCol = sl_browse.GetColumnName(lb_columns.value);
	
	if(strVal=="") return;
	if(strCol=="")return;
	
	
	var strFilter = "_swc_" + strCol + "=" + strVal;
	//-- apply passed in filter
	if(_swdoc.PassedInFilter!="") strFilter += "&" + _swdoc.PassedInFilter;
	
	_slf(sl_browse, strFilter);
	sl_browse.SetRowSelected(0);
	
	_een(btn_clear, true);

}]]>
						</code>
					</method>
					<method>
						<name>btn_clear_OnPressed</name>
						<code>
							<![CDATA[btn_clear_OnPressed()
{
	_ete(tb_lookfor, "");
		
	var strFilter = _swdoc.PassedInFilter;
	if(strFilter=="")strFilter="empty=1";
	
	_slf(sl_browse , strFilter);
	sl_browse.SetRowSelected(0);
	
	_een(btn_clear, false);


}]]>
