					<name>(Globals)</name>
					<code>
						<![CDATA[function oPricing()
{
	return _swdoc.contract_pricing;
}

function calculate_price()
{
	//-- recalculate the price by adjustments
	if(_swdoc.oPricing().stdprice!="")
	{
		if(_swdoc.oPricing().percadjustment!="")
		{
			if(_swdoc.oPricing().adjustmentdir=="+")
			{
				//MessageBox((_swdoc.oPricing().stdprice*1)/(_swdoc.oPricing().percadjustment*1))
				_swdoc.oPricing().adjustedprice = format_float_to_decimal_str((_swdoc.oPricing().stdprice*1) + (((_swdoc.oPricing().stdprice*1)/100)*(_swdoc.oPricing().percadjustment*1)));
			}
				
			if(_swdoc.oPricing().adjustmentdir=="-")
				_swdoc.oPricing().adjustedprice = format_float_to_decimal_str((_swdoc.oPricing().stdprice*1) - (((_swdoc.oPricing().stdprice*1)/100)*(_swdoc.oPricing().percadjustment*1)));
				
			_swdoc.UpdateFormFromData();
		}
	}
	
}]]>
					</code>
