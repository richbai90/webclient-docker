					<method>
						<name>btn_remove_step_OnPressed</name>
						<code>
							<![CDATA[btn_remove_step_OnPressed()
{
	var strConfirm = "Are you sure you want to delete the selected step?";
	var intStepPos = parseInt(sl_steps.GetItemTextRaw(sl_steps.curSel, "sindex"));
	
	if(app.g.sl_deldbrow(sl_steps,sl_steps.curSel,0,null,strConfirm))
	{
		var intStepId=0;
		//-- loop through list and set seq for each
		for(var x=0; x < sl_steps.rowCount();x++)
		{
			var intThisStepPos = parseInt(sl_steps.GetItemTextRaw(x, "sindex"));
			if(intThisStepPos > intStepPos)
			{
				intStepId = sl_steps.GetItemTextRaw(x, "pk_auto_id");
				var strParams = "sid="+app.pfu(intStepId);
				app.g.submitsqs("form/wssm_wiz/dec_stage_seq", strParams);
			}
		}
		sl_steps.Refresh();
		sl_steps.SetRowSelected(0, true, false);
	}
}]]>
						</code>
					</method>
					<method>
						<name>btn_ups_OnPressed</name>
						<code>
							<![CDATA[btn_ups_OnPressed()
{
	var currRow = sl_steps.curSel;
	var intDownID = sl_steps.GetItemTextRaw(sl_steps.curSel-1, 'pk_auto_id');
	
	//-- get id to up
	var intUpID = sl_steps.GetItemTextRaw(sl_steps.curSel, 'pk_auto_id');
	
	//-- update seq id for both 
	var strParams = "sid="+app.pfu(intUpID);
	if(app.g.submitsqs("form/wssm_wiz/dec_stage_seq", strParams))
	{
		var strParams = "sid="+app.pfu(intDownID);
		if(app.g.submitsqs("form/wssm_wiz/inc_stage_seq", strParams))
		{
			//-- refresh list
			sl_steps.Refresh();
			sl_steps.SetRowSelected(currRow-1, true, false);
		}
	}

}]]>
						</code>
					</method>
