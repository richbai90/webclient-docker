					<method>
						<name>lbl_startday_OnValueChanged</name>
						<code>
							<![CDATA[lbl_startday_OnValueChanged(strValue)
{
	// TODO: Add your event handler code here
	if(strValue != "")
	{
		if(_swdoc.BL().end_dayx != "" && _swdoc.BL().end_dayx != 0)
		{
			var oDates = new Object();
			oDates.endDatex = app.g.fix_epoch(_swdoc.BL().end_dayx);
			oDates.startDatex = app.g.fix_epoch(strValue);
			if(oDates.startDatex>oDates.endDatex)
			{
				MessageBox("Start Date cannot be after the End Day.");
				_swdoc.BL().start_dayx = "";
				_swdoc.UpdateFormFromData();
			}
		}
	}
}]]>
