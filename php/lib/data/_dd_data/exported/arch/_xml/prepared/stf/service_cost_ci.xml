					<method>
						<name>btn_add_customisable_OnPressed</name>
						<code>
							<![CDATA[btn_add_customisable_OnPressed()
{
	// TODO: Add your event handler code here
	app.cmdb.search_service(true,"",function(oRes)
	{
		if(oRes.selectedkeys=="")return;

		//-- remove duplicates
		var strKeys = "";
		var arrKeys = oRes.selectedkeys.split(",");
		for(var x=0;x<arrKeys.length;x++)
		{
			//-- not in list and not the item we actually have loaded  ok to add
			var foundRowNum = app.g.sqllist_findrow_byvalue(sl_custom,arrKeys[x],'fk_key')
			if((foundRowNum==-1)&&(arrKeys[x] != service_costs.pk_auto_id))
			{
			
				if(strKeys!="")strKeys +=",";
				strKeys += arrKeys[x];
			}
		}
		if(strKeys=="")return ;
		
		app.service.add_component_custom(service_costs.pk_auto_id,strKeys);
		sl_custom.Refresh();
		
		if(sl_custom.rowCount()>0)
		{
			if(service_costs.flg_cancustomise!="1") 
			{
				service_costs.flg_cancustomise="1";
				_swdoc.Save(0);
			}
		}
	});
}]]>
						</code>
					</method>
