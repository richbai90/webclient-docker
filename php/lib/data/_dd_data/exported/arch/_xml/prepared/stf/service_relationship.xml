	if(strType.toUpperCase()=="ME->SERVICE")
	{
		var strNewType = "";
		var oCI = app.g.get_record("CONFIG_ITEMI", lb_ci.text);
		if(oCI)
		{
			strNewType = oCI.catalog_type;
		}
		app._slf(lb_relation_type , "swc=fk_config_type&kvs="+app.pfu(strNewType)+"&sf=citypes_flg_src1");
	
	}
	else
	{
		app._slf(lb_relation_type , "swc=fk_config_type&kvs="+app.pfu(strType)+"&sf=citypes_flg_src1");
	}

	if(_swdoc.GetArg("comp")=='false')
	{
		app._evi(flg_optional,false);
	}

	//-- and get linked items and display
	app._slf(sl_configitems,"pids=" + _swdoc.GetArg("linkitems") );

	
