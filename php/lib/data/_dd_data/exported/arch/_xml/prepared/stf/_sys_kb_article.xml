					<method>
						<name>sl_cust_services1_OnItemChecked</name>
						<code>
							<![CDATA[sl_cust_services1_OnItemChecked(nRow, bChecked)
{
	
	if(!app.itsm.can_edit(OC().callclass,true))
	{
		sl_cust_services1.SetRowChecked(nRow, !bChecked);
		return false;
	}

	var pk_ci_id = sl_cust_services1.GetItemTextRaw(nRow,"pk_auto_id");
	var ci_item_key = sl_cust_services1.GetItemTextRaw(nRow,"ck_config_item");		

	if(bChecked)		
	{
		//-- Relate the CI to the call
			
		//-- make sure is not already related
		if(app.g.sl_findrow_byvalue(EF().sl_configitems, pk_ci_id, "pk_auto_id") < 0)
		{
			//-- relate the call
			if (app.cmdb.relate_ci_to_call(OC().callref,pk_ci_id,'INCIDENT'))
			{
				var strText = "Service (" + ci_item_key + ") was added to this Incident.\n"
				var strActionSource = "Incident Management";
				var strCode			= "Linking Services";
				
				app.g.new_dairyevent(OC().callref,strText,strActionSource,strCode);
				
				//-- filter list again
				app.cmdb.sl_load_call_cis(EF().sl_services, OC().callref ,"INCIDENT","(CK_CONFIG_TYPE LIKE 'SERVICE%' OR CK_CONFIG_TYPE LIKE 'ME->Service')");			
			}
		}
		else
		{
			MessageBox("The selected customer configuration item is already associated to this incident");
		}
	}	
	else
	{
		//-- remove
   		//-- NWJ - 28.02.2006 - Use CMDB object to remove CIs from call
   		if (app.cmdb.unrelate_ci_from_call(OC().callref,pk_ci_id,'INCIDENT'))
   		{
