					<method>
						<name>Button2_OnPressed</name>
						<code>
							<![CDATA[Button2_OnPressed()
{
	app.g.pick_problemprofile("", "", false, function(oProfileCode)
	{
		var strProfileCode = oProfileCode;
		
		if(strProfileCode!="")
		{
			if(strProfileCode!=_swdoc.MF().s_kb__callprobcode.text)
			{
				app._eva(_swdoc.MF().s_kb__callprobcode,strProfileCode);
				
				var x = app.g.get_record("pcdesc", strProfileCode);
				arrWords = x.info.split(" ");
				var strKeywords = _swdoc.MF().s_kb__keywords.text;
				if(strKeywords=="")
					var arrKeywords = new Array();
				else
				{
					var intReturn = MessageBox("You already have keywords defined for this KnowledgeBase document.\n\nTo merge the new keywords into the existing ones, click Yes\nTo overwrite the existing keywords, click No\nTo keep the existing keywords and ignore the new ones, click Cancel",35);
					if(intReturn==2)
						return;
					if(intReturn==6)
						var arrKeywords = strKeywords.split(",");
					else
						var arrKeywords = new Array();
				}
			
				for(var i = 0; i<arrWords.length; i++)
				{
					var thisWord = arrWords[i];
					if(thisWord=="->")
					{
					}else
					{
						if(!app.in_array(arrKeywords,thisWord))
						{
							arrKeywords.push(thisWord);
						}
					}
				}
				app._eva(_swdoc.MF().s_kb__keywords, arrKeywords.join(","));
			}
		}
	});
}]]>
						</code>
