						<name>OnFormLoaded</name>
						<code>
							<![CDATA[OnFormLoaded(bDataReloaded)
{
	//-- 12.07.2008 
	//-- if inserting see if we need to set default company
	if(_swdoc.oCostcent().costcenter=="")
	{
		_swdoc.oCostcent().fk_company_id = unescape(_swdoc.GetArg("fk_company_id"));		
		if((_swdoc.oCostcent().fk_company_id=="")&&(dd.GetGlobalParamAsString("Application Settings/SupportExternalCompanies")!=1))
		{
			_swdoc.oCostcent().fk_company_id = dd.GetGlobalParamAsString("Application Settings/DefaultCompanyID");
		}	
		_swdoc.UpdateFormFromData();	
	}
	else
	{
		//-- Filter Default SLAs & Call Classes
		app._een(mainform.lbl_sl, true);
		app._een(mainform.sl_defslas, true);
		app._een(mainform.btn_add_class, true);
		app._een(mainform.btn_rem_class, true);	
		app._slf(mainform.sl_defslas, "reltype=CHCT&fk_rel_id="+app.pfu(costcent.costcenter));
		mainform.sl_defslas.Refresh();
	}
	
	if(_swdoc.oCostcent().itsm_sla>0)
	{
		app._een(mainform.fld_priority, true);
		app._een(mainform.lbl_priority, true);
	}
	_swdoc.ResetModiedFlag("costcent");	
}]]>
						</code>
					</method>
