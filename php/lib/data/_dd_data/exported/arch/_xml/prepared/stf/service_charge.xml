function MF()
{
	return mainform;
}


function setup_form_fields()
{
	var boolAllowEdit = _swdoc.MF().fld_chargable.value==1;
	var svcPrice =_swdoc.GetArg("servicePrice");
	if(svcPrice!="")
	{
		_swdoc.servicePrice = svcPrice;
	}
	app._een(_swdoc.MF().fld_unit_charge, boolAllowEdit);
	app._een(_swdoc.MF().fld_mark_up, boolAllowEdit);
	app._een(_swdoc.MF().fld_units, boolAllowEdit);
	app._slf(_swdoc.MF().sl_items, "pids="+_swdoc.GetArg("linkitems"));
	_swdoc.MF().sl_items.Refresh();
	if(!boolAllowEdit)
		app._eva(_swdoc.MF().fld_charge_type, "N/A");
	var serviceItem = _swdoc.GetArg("serviceItem");
	var reqPrice = _swdoc.GetArg("reqPrice");
	if(reqPrice=="")
		reqPrice = "0.00";
	app._ete(_swdoc.MF().fld_request_price, reqPrice);
	var subPrice = _swdoc.GetArg("subPrice");
	if(subPrice=="")
		subPrice = "0.00";
	_swdoc.origValueReq = reqPrice;
	_swdoc.origValueSub = subPrice;
	app._ete(_swdoc.MF().fld_subscription_price, subPrice);//_swdoc.GetArg("subPrice");
	app._slf(_swdoc.MF().sl_service_sla1, "swc=fk_service&kvs="+serviceItem);
	_swdoc.MF().sl_service_sla1.Refresh();
	_swdoc.UpdateFormFromData();
}

function recalculate()
{
	if(_swdoc.MF().fld_unit_charge.value=="" || _swdoc.MF().fld_units.value =="")
		return;
		
	app._eva(_swdoc.MF().fld_total_charge, (_swdoc.roundNumber(parseFloat(_swdoc.MF().fld_unit_charge.value)*parseFloat(_swdoc.MF().fld_units.value)*((100+parseFloat(_swdoc.MF().fld_mark_up.value))/100),2)));
	app._ete(_swdoc.MF().fld_total_charge, _swdoc.MF().fld_total_charge.value);
	_swdoc.UpdateFormFromData();
}

function roundNumber(num, dec) {
	var result = Math.round(num*Math.pow(10,dec))/Math.pow(10,dec);
	return result;
}]]>
